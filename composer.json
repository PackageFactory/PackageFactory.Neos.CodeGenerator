{
    "name": "packagefactory/neos-codegenerator",
    "type": "neos-package",
    "description": "(Opinionated yet extensible) generator for common code patterns in Neos and Neos.Flow projects",
    "license": "MIT",
    "authors": [
        {
            "name": "Wilhelm Behncke",
            "email": "behncke@sitegeist.de",
            "role": "Developer"
        },
        {
            "name": "Bernhard Schmitt",
            "email": "schmitt@sitegeist.de",
            "role": "Developer"
        }
    ],
    "require": {
        "neos/neos": "~5.0 || dev-master",
        "phpstan/phpdoc-parser": "^0.4.9",
        "league/climate": "^3.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.3",
        "phpstan/phpstan": "^0.12.45",
        "neos/buildessentials": "^6.3",
        "mikey179/vfsstream": "^1.6",
        "spatie/enum": "~2.3",
        "spatie/phpunit-snapshot-assertions": "^4.2"
    },
    "config": {
        "vendor-dir": "Packages/Libraries",
        "bin-dir": "bin"
    },
    "autoload": {
        "psr-4": {
            "PackageFactory\\Neos\\CodeGenerator\\": "Classes"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PackageFactory\\Neos\\CodeGenerator\\Tests\\": "Tests"
        }
    },
    "extra": {
        "neos": {
            "package-key": "PackageFactory.Neos.CodeGenerator"
        }
    }
}
